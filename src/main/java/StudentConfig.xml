<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
<bean class="com.jspiders.springcore.bean.StudentBean"  id="student1">

<property name="id" value="1"/>
<property name="name" value="pranita"/>
<property name="email" value="pranita@gmail.com"/>
</bean>
<bean class="com.jspiders.springcore.bean.StudentBean" id="student2">
<constructor-arg name="id" value="2"/>
<constructor-arg name="name" value="swati"/>
<constructor-arg name="email" value="swati@gmail.com"/>
</bean>
 

</beans>
